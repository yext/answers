<div class="yxt-ResultsFooter">
  {{#if _config.url}} 
    <a href="{{_config.url}}"
      target={{#if _config.newWindow}}"_blank"
      rel="noopener noreferrer nofollow"{{else}}"_self"{{/if}}
      aria-label="Vertical Results footer link"
    >
    {{#if _config.screenReaderText}}
      <span class="sr-only">{{_config.screenReaderText}}</span>
    {{/if}}
  {{/if}}
  

  {{#if _config.image}}
    <img src="{{_config.image}}" {{#if _config.altText}} alt="{{_config.altText}}" {{/if}}>
  {{/if}}
  {{#if _config.icon}}
    <span data-component="IconComponent" data-opts='{ "iconName": "{{_config.icon}}" }'
    {{#if _config.altText}} alt="{{_config.altText}}" {{/if}}></span>
  {{/if}}

  {{#if _config.url}} 
    </a>
  {{/if}}
</div>