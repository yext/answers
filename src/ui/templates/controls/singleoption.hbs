{{#each options}}
  <input
    type="radio"
    class="js-yext-filter-option"
    id="js-yext-radio-{{../name}}-{{@index}}"
    name="{{../name}}"
    value="{{value}}"
    data-index="{{@index}}"
    {{#if checked}}checked{{/if}}>
  <label for="js-yext-radio-{{../name}}-{{@index}}">{{label}}</label>
{{/each}}
