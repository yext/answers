{{#if isSearchComplete}}
  {{#if showNoResults}}
    {{#if isEnhancedNoResultsEnabled}}
      <div class="yxt-Results-alternativeVerticals"
           data-component="AlternativeVerticals"></div>
    {{else}}
      {{> results/noresults}}
    {{/if}}
  {{/if}}
  {{#if resultsPresent}}
    <section class="yxt-Results yxt-Results--{{_config.modifier}}">
      {{#if _config.isUniversal}}
        {{> results/resultssectionheader}}
      {{else}}
        {{#if showResultsHeader}}
          <div data-component="ResultsHeader" data-opts='{{json _config.resultsHeaderOpts}}'></div>
        {{/if}}
      {{/if}}
      {{#if _config.includeMap}}
        <div class="yxt-Results-map"
             data-component="Map"
             data-prop="map">
        </div>
      {{/if}}
      <ul class="yxt-Results-items js-yxt-Results-items{{#if numColumns}} yxt-Results-items--{{numColumns}}{{/if}}">
        {{#each results}}
          <div class="yxt-Card" data-component="Card" data-opts='{"_index": {{@index}} }'></div>
        {{/each}}
        {{#each placeholders}}
          <div class="yxt-Results-placeholder yxt-Card" data-component="Card" data-opts='{"_index": 0 }' aria-hidden="true"></div>
        {{/each}}
      </ul>
    </section>
  {{/if}}
{{/if}}
