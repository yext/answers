<div class="yxt-ResultsHeader">
  {{#if _config.showResultCount}}
    {{#if resultsCount}}
      <div class="yxt-ResultsHeader-resultsCount">
        <span class="yxt-ResultsHeader-resultsCount-start">{{resultsCountStart}}</span>
        <span class="yxt-ResultsHeader-resultsCount-dash">-</span>
        <span class="yxt-ResultsHeader-resultsCount-end">{{resultsCountEnd}}</span>
        <span class="yxt-ResultsHeader-resultsCount-of">of</span>
        <span class="yxt-ResultsHeader-resultsCount-total">{{resultsCount}}</span>
      </div>
      {{#if showResultSeparator}}
        <div class="yxt-ResultsHeader-resultsCountSeparator">{{_config.resultsCountSeparator}}</div>
      {{/if}}
    {{/if}}
  {{/if}}
  {{#if shouldShowFilters}}
    <div class="yxt-ResultsHeader-appliedFilters">
      {{#each appliedFiltersArray}}
        {{#if ../_config.showFieldNames}}
          <div class="yxt-ResultsHeader-filterLabel">
            <span class="yxt-ResultsHeader-filterLabel-label">{{this.label}}</span>
            <span class="yxt-ResultsHeader-filterLabel-colon">:</span>
          </div>
        {{/if}}
        {{#each this.displayValues}}
          <div class="yxt-ResultsHeader-filterValue">{{this}}{{#unless @last}}, {{/unless}}</div>
        {{/each}}
        {{#unless @last}}
          <div class="yxt-ResultsHeader-filterSeparator">|</div>
        {{/unless}}
      {{/each}}
    </div>
  {{/if}}
</div>